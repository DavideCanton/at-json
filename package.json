{
    "name": "at-json",
    "version": "1.0.0",
    "description": "A declarative mapper to and from JSON.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prepublishOnly": "yarn build",
        "bench": "yarn build:test && node dist-test/test/benchmarks/benchmarks.js",
        "prebuild": "yarn clean",
        "build": "webpack",
        "clean": "rimraf dist coverage",
        "test": "jest",
        "test:coverage": "yarn test --collect-coverage",
        "show:coverage": "http-server ./coverage/lcov-report -o"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DavideCanton/at-json.git"
    },
    "keywords": [
        "json",
        "mapper"
    ],
    "author": "Davide Canton",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DavideCanton/at-json/issues"
    },
    "homepage": "https://github.com/DavideCanton/at-json#readme",
    "dependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@types/benchmark": "^1.0.31",
        "@types/jest": "^25.2.3",
        "@types/node": "^12.12.12",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "benchmark": "^2.1.4",
        "eslint": "^7.13.0",
        "eslint-plugin-import": "^2.22.1",
        "http-server": "^0.12.3",
        "jest": "^26.0.1",
        "jest-extended": "^0.11.5",
        "lodash": "^4.17.19",
        "rimraf": "^3.0.0",
        "ts-jest": "^26.0.0",
        "ts-loader": "^8.0.4",
        "ts-node": "^7.0.0",
        "tslib": "^1.11.1",
        "typescript": "^4.0.0",
        "webpack": "^5.64.4",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1"
    }
}
