{
    "name": "at-json",
    "version": "0.0.26",
    "description": "A declarative mapper to and from JSON.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prepublishOnly": "npm run build",
        "bench": "npm run build:test && node dist-test/test/benchmarks/benchmarks.js",
        "build": "tsc",
        "build:test": "tsc -b test/tsconfig.json",
        "clean": "rimraf dist && rimraf dist-test",
        "test": "nyc --reporter=html mocha -r ts-node/register test/**/test.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DavideCanton/at-json.git"
    },
    "keywords": [
        "json",
        "mapper"
    ],
    "author": "Davide Canton",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DavideCanton/at-json/issues"
    },
    "homepage": "https://github.com/DavideCanton/at-json#readme",
    "dependencies": {
        "core-js": "^3.4.2",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@types/benchmark": "^1.0.31",
        "@types/chai": "^4.1.4",
        "@types/chai-spies": "^1.0.0",
        "@types/mocha": "^5.2.5",
        "@types/node": "^12.12.12",
        "benchmark": "^2.1.4",
        "chai": "^4.1.2",
        "chai-spies": "^1.0.0",
        "mocha": "^7.1.0",
        "nyc": "^15.0.0",
        "rimraf": "^3.0.0",
        "ts-node": "^7.0.0",
        "tslint": "^5.17.0",
        "typescript": "^3.7.2"
    }
}
