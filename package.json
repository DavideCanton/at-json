    "dependencies": {},
    "devDependencies": {
        "@types/babel__core": "^7.20.2",
        "@types/benchmark": "^1.0.31",
        "@types/jest": "^29.1.1",
        "@types/node": "^12.12.12",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.22.1",
        "http-server": "^14.1.1",
        "jest": "^29.7.0",
        "jest-each": "^29.7.0",
        "jest-extended": "^4.0.1",
        "prettier": "^3.0.3",
        "rimraf": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "typedoc": "^0.25.1",
        "typescript": "^5.2.2",
        "webpack": "^5.64.4",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1"
