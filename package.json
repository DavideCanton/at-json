    "description": "A declarative mapper to and from JSON.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prepublishOnly": "npm run build",
        "prebuild": "npm run clean",
        "build": "webpack",
        "clean": "rimraf dist coverage",
